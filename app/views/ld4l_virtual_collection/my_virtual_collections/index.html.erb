<h1>Listing my_virtual_collections</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_virtual_collections.each do |my_virtual_collection| %>
      <tr>
        <td><%= link_to 'Show', my_virtual_collection %></td>
        <td><%= link_to 'Edit', edit_my_virtual_collection_path(my_virtual_collection) %></td>
        <td><%= link_to 'Destroy', my_virtual_collection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New My virtual collection', new_my_virtual_collection_path %>
